	</description>
	<files>
		<filename>component.php</filename>
		<filename>error.php</filename>
		<filename>favicon.ico</filename>
		<filename>index.php</filename>
		<filename>templateDetails.xml</filename>
		<filename>template_preview.png</filename>
		<filename>template_thumbnail.png</filename>
		<folder>css</folder>
		<folder>fields</folder>
		<folder>html</folder>
		<folder>images</folder>
		<folder>fonts</folder>
		<folder>scripts</folder>
		<folder>language</folder>
	</files>
	<positions>
		<position>debug</position>
		<position>mainnav</position>
		<position>top-search</position>
		<position>top-cart</position>
		<position>footer</position>
		<position>breadcrumbs</position>
		<position>right</position>
	</positions>
	<languages folder="language">
		<language tag="en-GB">en-GB/en-GB.tpl_jf_startupx.ini</language>
		<language tag="en-GB">en-GB/en-GB.tpl_jf_startupx.sys.ini</language>
	</languages>
	<config>
		<fields name="params">
			<fieldset name="advanced" addfieldpath="/templates/jf_startupx/fields/">
				<field name="logoOption" type="list" default="2"
					label="TPL_JF_STARTUPX_LOGO_OPTION_LABEL"
					description="TPL_JF_STARTUPX_LOGO_OPTION_DESC">
						<option value="1">TPL_JF_STARTUPX_LOGO_AS_IMAGE</option>
  						<option value="2">TPL_JF_STARTUPX_LOGO_AS_TEXT</option>
				</field>
				
				<field name="logoFile" class="" type="media" default=""
					label="TPL_JF_STARTUPX_LOGO_LABEL"
					description="TPL_JF_STARTUPX_LOGO_DESC" />
					
				<field name="logoText" type="text" default="StartupX"
					label="TPL_JF_STARTUPX_LOGO_TEXT_LABEL"
					description="TPL_JF_STARTUPX_LOGO_TEXT_DESC"
					filter="string" />
					
				<field name="backgroundColor" type="list" default=""
					label="TPL_JF_STARTUPX_COLOR_LABEL"
					description="TPL_JF_STARTUPX_COLOR_DESC">
						<option value="light">TPL_JF_STARTUPX_LIGHT</option>
  						<option value="dark">TPL_JF_STARTUPX_DARK</option>
				</field>
					
				<field name="googleFont" type="list" default="" label="TPL_JF_STARTUPX_FONT_LABEL" description="TPL_JF_STARTUPX_FONT_DESC">
					<option value="">-- Use default --</option>
					<option value="OpenSans">Open Sans</option>
					<option value="Lato">Lato</option>
					<option value="PTSans">PT Sans</option>
					<option value="SourceSansPro">Source Sans Pro</option>
					<option value="Exo2">Exo 2</option>
					<option value="Ubuntu">Ubuntu</option>
					<option value="IstokWeb">Istok Web</option>
					<option value="Nobile">Nobile</option>
				</field>

				<field name="templateColor" class="" type="color" default="#f97a3f"
					label="TPL_JF_STARTUPX_LINK_BUTTON_COLOR_LABEL"
					description="TPL_JF_STARTUPX_LINK_BUTTON_COLOR_DESC" />				
					
				<field type="spacer" name="myspacer1" hr="true" />
					
				<field name="social" 
					type="radio" 
					class="btn-group btn-group-yesno"
					default="1"
					label="Show Social Button"
					description="Select an option">
						<option value="1">JYES</option>
						<option value="0">JNO</option>
				</field>
				
				<field name="twitterIcon" 
					type="radio" 
					class="btn-group btn-group-yesno"
					default="1"
					label="Show Twitter Icon"
					description="Select an option">
						<option value="1">JYES</option>
						<option value="0">JNO</option>
				</field>
				
				<field name="twitterLink" type="text" default=""
					label="Twitter Link"
					description="Please add your twitter link here"
					filter="string" />
				
				<field name="facebookIcon" type="radio" class="btn-group btn-group-yesno" default="1"
					label="Show Facebook Icon"
					description="Select an option">
						<option value="1">JYES</option>
						<option value="0">JNO</option>
				</field>
				
				<field name="facebookLink" type="text" default=""
					label="Facebook Link"
					description="Please add your facebook link here"
					filter="string" />
					
				<field name="linkedInIcon" type="radio" class="btn-group btn-group-yesno" default="1"
					label="Show LinkedIn Icon"
					description="Select an option">
						<option value="1">JYES</option>
						<option value="0">JNO</option>
				</field>
				
				<field name="linkedInLink" type="text" default=""
					label="LinkedIn Link"
					description="Please add your linkedIn link here"
					filter="string" />
					
				<field name="googleIcon" type="radio" class="btn-group btn-group-yesno" default="1"
					label="Show Google Icon"
					description="Select an option">
						<option value="1">JYES</option>
						<option value="0">JNO</option>
				</field>
				
				<field name="googleLink" type="text" default=""
					label="Google Link"
					description="Please add Google link here"
					filter="string" />									
					
				<field name="pinterestIcon" type="radio" class="btn-group btn-group-yesno" default="1"
					label="Show Pinterest Icon"
					description="Select an option">
						<option value="1">JYES</option>
						<option value="0">JNO</option>
				</field>
				
				<field name="pinterestLink" type="text" default=""
					label="Pinterest Link"
					description="Please add your Pinterest link here"
					filter="string" />
					
				<field name="youtubeIcon" type="radio" class="btn-group btn-group-yesno" default="1"
					label="Show Youtube Icon"
					description="Select an option">
						<option value="1">JYES</option>
						<option value="0">JNO</option>
				</field>
				
				<field name="youtubeLink" type="text" default=""
					label="Youtube Link"
					description="Please add your Youtube link here"
					filter="string" />				
					
				<field type="spacer" name="myspacer2" hr="true" />
				
				<field name="map" type="radio" default="1"
					label="Use map in contact"
					description="Select an option">
						<option value="0">No</option>
  						<option value="1">Yes</option>
				</field>
				
				<field name="gmapAPIKey" type="text" default=""
					label="Google Maps API key"
					description="Google Maps API key"
					filter="string" />
					
				<field name="latitude" type="text" default="46.4689782"
					label="Latitude"
					description="Latitude"
					filter="string" />
					
				<field name="longitude" type="text" default="11.329879"
					label="Longitude"
					description="Longitude"
					filter="string" />
					
				<field name="longlat" type="map" label="Google Map" description="" ></field>

			</fieldset>
		</fields>
	</config>
</extension>
